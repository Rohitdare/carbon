# Server Configuration
PORT=8000
HOST=0.0.0.0
DEBUG=True

# Database Configuration
DATABASE_URL=postgresql://postgres:password@postgres:5432/blue_carbon_mrv
REDIS_URL=redis://redis:6379

# Google Earth Engine Configuration
GEE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GEE_PRIVATE_KEY_PATH=/app/credentials/gee-private-key.json
GEE_PROJECT_ID=your-gcp-project-id

# Model Configuration
MODEL_UPDATE_INTERVAL=3600  # seconds
MODEL_RETRAIN_THRESHOLD=1000  # new data points
CARBON_ESTIMATION_CONFIDENCE_THRESHOLD=0.8

# API Configuration
BACKEND_API_URL=http://backend:3000
API_KEY=your-api-key-here
CORS_ORIGINS=["http://localhost:3000", "http://web-dashboard:3000"]

# Logging
LOG_LEVEL=INFO
LOG_FILE=/app/logs/ai-ml.log

# File Storage
UPLOAD_DIR=/app/data/uploads
MODEL_DIR=/app/models
TEMP_DIR=/app/data/temp

